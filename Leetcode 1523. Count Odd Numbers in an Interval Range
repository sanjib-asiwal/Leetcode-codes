//@CodeVed
class Solution {//@CodeVed
public://@CodeVed
    int countOdds(int low, int high) {
        //@CodeVed
        if(low%2 || high%2)
            return ((high-low)/2) + 1; 
        return ((high-low)/2);
        //@CodeVed
    //@CodeVed
    //@CodeVed
    }//@CodeVed
};//@CodeVed
//@CodeVed
//@CodeVed


class Solution {//@CodeVed
public://@CodeVed
    int countOdds(int low, int high) {
        int cnt=0;
        for(int i=low;i<=high;i++){
            if(i%2!=0) cnt++;
        }//@CodeVed
        return cnt;
    }//@CodeVed
};//@CodeVed
