class Solution {//@CodeVed
public:
    int countOdds(int low, int high) {
        if (!(low & 1)) {
            low++;
        }//@CodeVed
        return low > high ? 0 : (high - low) / 2 + 1;
    }//@CodeVed
};//@CodeVed

//@CodeVed//@CodeVed
//@CodeVed//@CodeVed
//@CodeVed//@CodeVed
